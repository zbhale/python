pyg = 'ay'

original = raw_input('Enter a word:')
original = original.lower()

if len(original) > 0 and original.isalpha() and original[0] != "a" and original[0] != "e" and original[0] != "i" and original[0] != "o" and original[0] != "u":
    first= original[0]
    new_word = original+first+pyg
    new_word = new_word[1:len(new_word)]
    new_word.capitalize()
    
    print new_word
    
elif len(original) > 0 and original.isalpha():
    new_word = original+pyg
    new_word.capitalize()
    
    print new_word

else:
    print 'empty'
